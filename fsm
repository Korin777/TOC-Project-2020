digraph "State Machine" {
	graph [color=black directed=true fillcolor=white label="State Machine" rankdir=LR "strict"=false style=solid]
	node [color=black fillcolor=white peripheries=1 shape=rectangle style="rounded, filled"]
	edge [color=black]
	initial [label=initial color=red fillcolor=darksalmon peripheries=2]
	menu [label=menu]
	pixiv [label=pixiv]
	find_pixiv_id [label=find_pixiv_id]
	initial -> menu [label="advance [is_going_to_menu]"]
	menu -> pixiv [label="advance [is_going_to_pixiv]"]
	pixiv -> find_pixiv_id [label="advance [is_going_to_find_pixiv_id]"]
	pixiv -> menu [label="advance [is_going_to_menu]"]
	pixiv -> initial [label=go_back]
	find_pixiv_id -> menu [label="advance [is_going_to_menu]"]
	find_pixiv_id -> pixiv [label=back_pixiv]
}
